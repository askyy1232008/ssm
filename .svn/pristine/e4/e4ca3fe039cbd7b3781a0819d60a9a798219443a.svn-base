			</if>
			<if test="loanpeople != null">
				and POSITION(#{loanpeople,jdbcType=VARCHAR} IN lc.loanpeople)>0
			</if>
		</where>
		order by lc.id desc
		limit #{offset},#{limit} 
	</select>
